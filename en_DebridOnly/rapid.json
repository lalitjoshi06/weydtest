{
  "name": "RapidMoviez",
  "language": [    
    "en"
  ],
  "domains": [
    "rapidmoviez.unblockit.buzz"
  ],
  "base_url": "https://rapidmoviez.unblockit.buzz",
  "search_url_format_episode": {
    "string_format": "/search/%s+s%02de%02d",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/search/%s+%04d",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/search/=%s",
    "replacement": [
      "title_lower"
    ]
  },
  "more_pages_indicator": "",
  "next_page_path" : "",
  "next_page_path_attr": "",
  "links_on_first_page": false,
  "drill_down_path": "div.lists_items ul li a[href^=/release/]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": false,
  "is_direct": true,
  "links_container_path": "div.blog-details",
  "link_path": "pre.links a",
  "link_path_attr": "href",
  "title_path": "h2",
  "title_path_attr": "title",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "",
  "magnet_path_attr": "",
  "seeders_path": "",
  "seeders_path_attr": "",
  "filesize_path": "",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}