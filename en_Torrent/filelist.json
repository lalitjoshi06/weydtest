{
  "name": "Filelisting",
  "language": [
    "hi",
    "en"
  ],
  "domains": [
    "filelisting.com"    
  ],
  "base_url": "https://filelisting.com",
  "search_url_format_episode": {
    "string_format": "/result?q=%s+s%02de%02d",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/result?q=%s+%04d",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/result?q=%s",
    "replacement": [
      "title_lower"
    ]
  },
  "more_pages_indicator": "",
  "next_page_path": "",
  "next_page_path_attr": "",
  "links_on_first_page": false,
  "drill_down_path": "td.dn-btn a[href^=/]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div.well",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "table:eq(1) tr td:eq(1) a[title]",
  "title_path_attr": "title",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "table:eq(1) tr td:eq(2) a#download-btn",
  "magnet_path_attr": "",
  "seeders_path": "",
