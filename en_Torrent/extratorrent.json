{
	"name": "extratorrent",
	"language": ["en"],
	"domains": ["extratorrent.unblockit.ltd"],
	"base_url": "http://extratorrent.unblockit.ltd",
	"search_url_format_episode": {
		"string_format": "/search/?search=%s+s%02de%02d&new=1&x=0&y=0",
		"replacement": [
			"title_lower",
			"season_int",
			"episode_int"
		]
	},
	"search_url_format_movie": {
		"string_format": "/search/?search=%s+%04d&new=1&x=0&y=0",
		"replacement": [
			"title_lower",
			"year_int"
		]
	},
	"search_url_format_season_pack": {
		"string_format": "/search/?search==%s&new=1&x=0&y=0",
		"replacement": [
			"title_lower"
		]
	},
	"api":{
    "path_title": "name",
    "path_hash": "info_hash",
    "path_seeders": "seeders",
    "path_size": "size"
  },

  "is_torrent": true,
  "is_direct": false
}
