{
  "name": "magnetz",
  "language": ["en", "hi"],
  "domains": ["magnetz.eu"],
  "base_url": "https://magnetz.eu",
  "search_url_format_episode": {
    "string_format": "/search?q=%s+S%02dE%02d",
    "replacement": ["title", "season_int", "episode_int"]
  },
  "search_url_format_movie": {
    "string_format": "/search?q=%s+%04d",
    "replacement": ["title", "year_int"]
  },
  "search_url_format_season_pack": {
    "string_format": "/search?q=%s",
    "replacement": ["title_lower"]
  },
  "check_title_before_drill_down": true,
  "drill_down_title_check_attr": "|HTML|",
  "max_more_pages": 10,
  "more_pages_indicator": "",
  "next_page_path": "",
  "next_page_path_attr": "",
  "links_on_first_page": false,
  "drill_down_path": "div.list-group a.list-group-item[href]",
  "drill_down_path_attr": "abs:href",
  "links_container_path": "div.text-start",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "h4",
	"title_path_attr": "",
	"details_path": "|title_path|",
	"details_path_attr": "",
  "magnet_path": "a[href^=magnet]",
  "magnet_path_attr": "href",
  "seeders_path": "",
  "seeders_path_attr": "",
  "filesize_path": "dl.row dt:contains('Total Size') + dd",
  "filesize_path_attr": "",
  "quality_path": "div.text-start h4",
  "quality_path_attr": "",
  "info_path": "div.text-start h4",
  "info_path_attr": ""
  
}
