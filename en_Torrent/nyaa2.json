{
	"name": "nyaa2",
	"language": ["en"],
	"domains": ["nyaa.net"],
	"base_url": "https://nyaa.net",
	"search_url_format_episode": {
		"string_format": "/api/search?c=3_5&q=s+s%02de%02d&refine=1",
		"replacement": [
			"title_lower",
			"season_int",
			"episode_int"
		]

	},

	"search_url_format_movie": {
		"string_format": "/api/search?c=3_5&q=%s+%04d&refine=1",
		"replacement": [
			"title_lower",
			"year_int"
		]
	},
	"search_url_format_season_pack": {
		"string_format": "/api/search?c=3_5&q=%s&refine=1",
		"replacement": [
			"title_lower"
		]
	},
	"api": {
    "path_to_array": "torrents",
    "path_title": "name",
    "path_seeders": "seeders",
    "path_size": "filesize",
    "path_magnet": "magnet"
  },
  "is_torrent": true,
  "is_direct": false
}
