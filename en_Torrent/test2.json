{
  "name": "mejortorrents1",
  "language": ["es", "en"],
  "domains": ["mejortorrents1.com"],
  "base_url": "https://mejortorrents1.com",  
  "search_url_format_episode": {
    "string_format": "/secciones.php?sec=buscador&valor=%s+s%02de%02d",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/secciones.php?sec=buscador&valor=%s+%04d",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/secciones.php?sec=buscador&valor=%s",
    "replacement": [
      "title_lower"
    ]
},
    "more_pages_indicator": "<ul class=\"pagination\">",
    "next_page_path" : "ul.pagination li.active + li a",
    "next_page_path_attr": "href",
    "links_on_first_page": false,
    "drill_down_path": "span.tv1 +a",
    "drill_down_path_attr": "abs:href",
    "is_torrent": true,
    "is_direct": false,
    "links_container_path": "a[href^=magnet]",
    "link_path": "",
    "link_path_attr": "",
    "title_path": "/dl:contains(name) dd",
    "title_path_attr": "",
    "details_path": "|title_path|",
    "details_path_attr": "",
    "magnet_path": "a[href^=magnet]",
    "magnet_path_attr": "href",
    "seeders_path": "/dl:contains(swarm) dd b:eq(0)",
    "seeders_path_attr": "",
    "filesize_path": "/dl:contains(size) dd",
    "filesize_path_attr": "",
    "quality_path": "|title_path|",
    "quality_path_attr": "",
    "info_path": "|title_path|",
    "info_path_attr": ""
  }
  
