{
	"name": "BitSearch",
	"language": ["hi", "en"],
	"domains": ["bitsearch.to"],
	"base_url": "https://bitsearch.to",
	"search_url_format_episode": {
		"string_format": "/search?q=%s+s%02de%02d&sort=date&order=desc",
		"replacement": [
			"title_lower",
			"season_int",
			"episode_int"
		]
	},
	"search_url_format_movie": {
		"string_format": "/search?q=%s+%04d&sort=date&order=desc",
		"replacement": [
			"title_lower",
			"year_int"
		]
	},	
	"more_pages_indicator": "",
	"next_page_path": "",
	"next_page_path_attr": "",
	"links_on_first_page": false,
	"drill_down_path": "div.info h5.title a[href^=/torrents/]",
	"drill_down_path_attr": "abs:href",
	"is_torrent": true,
	"is_direct": false,
	"links_container_path": "div.details-box",
	"link_path": "",
	"link_path_attr": "",
	"title_path": "div.details h1.mt-0",
	"title_path_attr": "",
	"details_path": "|title_path|",
	"details_path_attr": "",
	"magnet_path": "div.dl-links a[href^=magnet]",
	"magnet_path_attr": "href",
	"seeders_path": "table.stat-table tr:eq(0) td:eq(1)",
	"seeders_path_attr": "",
	"filesize_path": "table.stat-table tr:eq(2) td:eq(1)",
	"filesize_path_attr": "",	
	"quality_path": "|title_path|",
	"quality_path_attr": "",
	"info_path": "|title_path|",
	"info_path_attr": ""
}
