	],
	"domains": ["torrentapi-f16xxden.b4a.run"],
  "base_url": "https://torrentapi-f16xxden.b4a.run",  
  "search_url_format_episode": {
    "string_format": "/api/limetorrent/%s+s%02de%02d",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/api/limetorrent/=%s+%04d",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/api/limetorrent/%s+hindi",
    "replacement": [
      "title_lower"
    ]
  },
  "api":{
    "path_to_array": "data",
