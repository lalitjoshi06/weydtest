{
	"name": "Bitsearch-API",
	"language": [
		"en",
		"hi"
	],
	"domains": ["torrent-api06.herokuapp.com"],
  "base_url": "https://torrent-api06.herokuapp.com",  
  "search_url_format_episode": {
    "string_format": "/api/bitsearch/%s+s%02de%02d+hindi",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/api/bitsearch/%s+%04d+hindi",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/api/bitsearch/%s+hindi",
    "replacement": [
      "title_lower"
    ]
  },
  "api":{
    "path_title": "Name",    
    "path_seeders": "Seeders",
	  "path_size_mb": "Size",
    "path_size_gb": "Size",	   
	  "path_magnet": "Magnet"
  },

  "is_torrent": true,
  "is_direct": false
}
