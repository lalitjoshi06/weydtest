{
  "name": "T-Paradise",
  "language": [
    "hi",
    "en"
  ],
  "domains": [
    "torrentparadise.org"
  ],
  "base_url": "https://torrentparadise.pm",
  "search_url_format_episode": {
    "string_format": "/search.php?f=%s+s%02de%02d",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/search.php?f=%s+%04d",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  }, 
  "more_pages_indicator": "",
  "next_page_path": "",
  "next_page_path_attr": "",
  "links_on_first_page": false,
  "drill_down_path": "tr.table-default td a[href^=/torrent/]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div.p-3",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "div.header h1",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": " a[href^=magnet]",
  "magnet_path_attr": "href",
  "seeders_path": "div.row span.seeders",
  "seeders_path_attr": "",
  "filesize_path": "div#cTree tbody tr + tr td span",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
