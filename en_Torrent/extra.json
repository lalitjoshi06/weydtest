{
  "name": "Ttz2",
  "language": ["hi", "en"],
  "domains": ["torrentz2beta.com"],
  "base_url": "https://torrentz2beta.com",
  "search_url_format_episode": {
    "string_format": "/beta/search.php?q=%s+s%02de%02d",
    "replacement": [
      "title",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/beta/search.php?q=%s+%04d",
    "replacement": [
      "title",
      "year_int"
    ]
  },  
  "more_pages_indicator": "NEXT",
  "next_page_path" : "",
  "next_page_path_attr": "",
  "links_on_first_page": false,
  "drill_down_path": "table#torrent-table tbody tr td:eq(5)",
  "drill_down_path_attr": "abs:href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div.pusher",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "/title",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "div.ui a[href^=magnet]",
  "magnet_path_attr": "href",
  "seeders_path": "tr td:eq(3)",
  "seeders_path_attr": "",
  "filesize_path": "tr td:eq(4)",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
