{
    "name": "EXT",
    "language": ["hi", "en"],
    "domains": ["ext.torrentbay.to"],
    "base_url": "https://ext.torrentbay.to",
    "search_url_format_episode": {
      "string_format": "/search/?q=%s-s%02de%02d",
      "replacement": [
        "title_lower",
        "season_int",
        "episode_int"
      ]
    },
    "search_url_format_movie": {
      "string_format": "/search/?q=%s-%04d",
      "replacement": [
        "title_lower",
        "year_int"
      ]
    },
    "more_pages_indicator": "Next page",
    "next_page_path" : "div.pagination-block div.page-link",
    "next_page_path_attr": "abs:href",
    "links_on_first_page": false,
    "drill_down_path": "div.float-left a[href]",
    "drill_down_path_attr": "abs:href",
    "is_torrent": true,
    "is_direct": false,
    "links_container_path": "div.card-body",
    "link_path": "",
    "link_path_attr": "",
    "title_path": "/title",
    "title_path_attr": "",
    "details_path": "|title_path|",
    "details_path_attr": "",
    "magnet_path": "a[href^=magnet]:eq(0)",
    "magnet_path_attr": "href",
    "seeders_path": "",
    "seeders_path_attr": "",
    "filesize_path": "",
    "filesize_path_attr": "",
    "quality_path": "|title_path|",
    "quality_path_attr": "",
    "info_path": "|title_path|",
    "info_path_attr": ""
  }
