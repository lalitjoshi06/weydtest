{
  "name": "Bull",
  "language": [
    "hi",
    "en"
  ],
  "domains": [
    "toros2.com"
  ],
  "base_url": "https://www.bulltorrent.com",
  "search_url_format_episode": {
    "string_format": "/television/torrents/%s-s%02de%02d.html?v=&i=100&sort=added&o=desc",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/movie/torrents/%s-%04d.html?v=&i=100&sort=added&o=desc",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "more_pages_indicator": "",
  "next_page_path": "",
  "next_page_path_attr": "",
  "links_on_first_page": false,
  "drill_down_path": "table.table-responsive tr td a[href^=/torrent/]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div.content",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "tbody tr h2",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "div.row a[href^=magnet]",
  "magnet_path_attr": "href",
  "seeders_path": "table.table-striped tr:eq(4) td:eq(1)",
  "seeders_path_attr": "",
  "filesize_path": "table.table-striped tr:eq(2) td:eq(1)",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
