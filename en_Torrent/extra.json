{
  "name": "btdb",
  "language": [
    "hi",
    "en"
  ],
  "domains": [
    "btdb.unblockit.buzz",
    "btdb.eu"
  ],
  "base_url": "https://btdb.unblockit.buzz",
  "search_url_format_episode": {
    "string_format": "/search/%s+s%02de%02d/0/?sort=popular",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/search/%s+%04d/0/?sort=popular",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/search/%s/0/?sort=popular",
    "replacement": [
      "title_lower"
    ]
  },
  "more_pages_indicator": "Next",
  "next_page_path": "div.nav ul.pagination li.next a",
  "next_page_path_attr": "abs:href",
  "links_on_first_page": true,
  "drill_down_path": "",
  "drill_down_path_attr": "",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div.card-body div.media",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "h2.item-title a[title]",
  "title_path_attr": "title",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "a[href^=magnet]",
  "magnet_path_attr": "href",
  "seeders_path": "small:contains(Seeders) strong",
  "seeders_path_attr": "",
  "filesize_path": "small:contains(Size) strong",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
