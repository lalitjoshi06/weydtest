{
	"name": "MGD",
	"language": [
		"en",
		"hi"
	],
	"domains": [
		"magnetdownload.com"
		
	],
	"base_url": "https://www.magnetdownload.com",
	"search_url_format_episode": {
		"string_format": "/search/%s+S%02dE%02d",
		"replacement": [
			"title",
			"season_int",
			"episode_int"
		]
	},
	"search_url_format_movie": {
		"string_format": "/search/%s+%04d",
		"replacement": [
			"title",
			"year_int"
		]
	},
	"search_url_format_season_pack": {
		"string_format": "/search/%s",
		"replacement": [
			"title_lower"
		]
	},
	"check_title_before_drill_down": true,
	"drill_down_title_check_attr": "|HTML|",
	"max_more_pages": 10,
	"more_pages_indicator": "",
	"next_page_path": "",
	"next_page_path_attr": "abs:href",
	"links_on_first_page": false,
	"drill_down_path": "tbody tr td.x-item a[href^=/info]",
	"drill_down_path_attr": "abs:href",
	"is_torrent": true,
	"is_direct": false,
	"links_container_path": "div.container",
	"link_path": "",
	"link_path_attr": "abs:href",
	"title_path": "h4",
	"title_path_attr": "",
	"details_path": "|title_path|",
	"details_path_attr": "",
	"magnet_path": "tr.tr-magnet-link a.magnet-link",
	"magnet_path_attr": "href",
	"seeders_path": "",
	"seeders_path_attr": "",
	"filesize_path": "table tr:has(th:contains(File Size)) td",
	"filesize_path_attr": "",
	"quality_path": "|title_path|",
	"quality_path_attr": "",
	"info_path": "|title_path|",
	"info_path_attr": ""
}
