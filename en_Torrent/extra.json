{
	"name": "tvsplurge",
	"language": ["hi", "en"],
	"domains": ["tvsplurge.io"],
	"base_url": "https://tvsplurge.io",
	"search_url_format_episode": {
		"string_format": "/shows/%s/season-/episode-/",
		"replacement": [
			"title_lower_dash",
			"season_int",
			"episode_int"
		]
	},
	"search_url_format_movie": {
		"string_format": "",
		"replacement": [
			"title_lower",
			"year_int"
		]
	},
	"search_url_format_season_pack": {
		"string_format": "/shows/%s/",
		"replacement": [
			"title_lower"
		]
	},
	"more_pages_indicator": "",
	"next_page_path": "",
	"next_page_path_attr": "",
	"links_on_first_page": true,
	"drill_down_path": "",
	"drill_down_path_attr": "",
	"is_torrent": true,
	"is_direct": false,
	"links_container_path": "table.torrents-list tr.torrent-row",
	"link_path": "",
	"link_path_attr": "",
	"title_path": "td.torrent_url a[data-title]",
	"title_path_attr": "",
	"details_path": "|title_path|",
	"details_path_attr": "",
	"magnet_path": "td.magnet_url a[href^=magnet]",
	"magnet_path_attr": "href",
	"seeders_path": "",
	"seeders_path_attr": "",
	"filesize_path": "",
	"filesize_path_attr": "",
	"filesize_units": "",
	"quality_path": "|title_path|",
	"quality_path_attr": "",
	"info_path": "|title_path|",
	"info_path_attr": ""
}
