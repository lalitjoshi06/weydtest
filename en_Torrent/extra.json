{
	"name": "CloudTorrent",
	"language": [
		"en",
		"hi"
	],
	"domains": [
		"cloudtorrents.com"
	],
	"base_url": "https://cloudtorrents.com",
	"search_url_format_episode": {
		"string_format": "/search?ordering=-uploaded&query=%s+S%02dE%02d",
		"replacement": [
			"title",
			"season_int",
			"episode_int"
		]
	},
	"search_url_format_movie": {
		"string_format": "/search?ordering=-uploaded&query=%s+%04d",
		"replacement": [
			"title",
			"year_int"
		]
	},
	"search_url_format_season_pack": {
		"string_format": "/search?ordering=-uploaded&query=%s",
		"replacement": [
			"title_lower"
		]
	},
	"check_title_before_drill_down": true,
        "drill_down_title_check_attr": "h1.title",
        "max_more_pages": 10
	"more_pages_indicator": "a.pagination-next",
	"next_page_path": "a.pagination-next",
	"next_page_path_attr": "abs:href"
	"links_on_first_page": false,
	"drill_down_path": "tbody tr td a[href^=/]",
	"drill_down_path_attr": "abs:href",
	"is_torrent": true,
	"is_direct": false,
	"links_container_path": "div.column",
	"link_path": "",
	"link_path_attr": "",
	"title_path": "h1.title",
	"title_path_attr": "",
	"details_path": "|title_path|",
	"details_path_attr": "",
	"magnet_path": "a[href^=magnet]",
	"magnet_path_attr": "href",
	"seeders_path": "div.columns div.column.is-half.is-table-cell:eq(0) span",
	"seeders_path_attr": "",
	"filesize_path": "div.columns div.column.is-half.is-table-cell:eq(3) span",
	"filesize_path_attr": "",
	"quality_path": "|title_path|",
	"quality_path_attr": "",
	"info_path": "|title_path|",
	"info_path_attr": ""
}
