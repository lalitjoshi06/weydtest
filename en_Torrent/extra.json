  "search_url_format_episode": {
    "string_format": "/search/%s+s%02de%02d/?c=&s=length",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/search/%s+%04d/?c=&s=length",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/search/%s/?c=&s=length",
    "replacement": [
      "title_lower"
    ]
  },
