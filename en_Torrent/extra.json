{
  "name": "TP2",
  "language": [
    "en"
  ],
  "domains": [
    "torrentproject2.com"
  ],
  "base_url": "https://torrentproject2.com",
  "search_url_format_episode": {
    "string_format": "/?t=%s+s%02de%02d&orderby=seeders",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/?t=%s+%04d&orderby=seeders",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "more_pages_indicator": "",
  "next_page_path": "table#nav tbody tr td + td a",
  "next_page_path_attr": "abs:href",
  "links_on_first_page": false,
  "drill_down_path": "div.tt span a[href^=/t3]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div#res2",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "div.usite a[title]",
  "title_path_attr": "title",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "div.usite a[href*=magnet]",
  "magnet_path_attr": "href",
  "seeders_path": "div#seeders-leechers",
  "seeders_path_attr": "",
  "filesize_path": "div#torrent-size",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
