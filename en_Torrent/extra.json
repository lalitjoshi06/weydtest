{
  "name": "oxtorrent",
  "language": [
    "en"
  ],
  "domains": [
    "oxtorrent.ws",
    "oxtorrent.io"
  ],
  "base_url": "https://www.oxtorrent.co",
  "search_url_format_episode": {
    "string_format": "/recherche/%s+s%02de%02d",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/recherche/%s+%04d",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "more_pages_indicator": "Suivant",
  "next_page_path": "ul.pagination li:contains('Suivant') a",
  "next_page_path_attr": "href",
  "links_on_first_page": false,
  "drill_down_path": "tbody tr td a[href^=/torrent/]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div.listing-detail",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "div.maximum",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "div.btn-magnet a[href^=magnet]",
  "magnet_path_attr": "href",
  "seeders_path": "table:eq(1) tbody tr:contains(Seeders) td font#retourSeeds",
  "seeders_path_attr": "",
  "filesize_path": "table:eq(1) tbody tr:eq(2)",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
