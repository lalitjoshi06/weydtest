{
	"name": "Limetorrent-API",
	"language": [
		"en",
		"hi"
	],
	"domains": ["torrent-api06.herokuapp.com"],
  "base_url": "https://torrent-api-py-mope.onrender.com",  
  "search_url_format_episode": {
    "string_format": "/api/v1/search?site=limetorrent&query=%s+s%02de%02d",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/api/v1/search?site=limetorrent&query=%s+%04d",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/api/v1/search?site=limetorrent&query=%s+hindi",
    "replacement": [
      "title_lower"
    ]
  },
  "api":{
    "path_title": "name",    
    "path_seeders": "seeders",	  
    "path_size": "size",
	"path_size_gb": "size",
	  "path_magnet": "magnet"
  },

  "is_torrent": true,
  "is_direct": false
}
