{
    "name": "torrentgalaxy",
    "language": ["hi", "en"],
    "domains": ["torrentgalaxy.to", "torrentgalaxy.mx", "torrentgalaxy.unblockedfor.me"],
    "base_url": "https://tgx.rs",
    "search_url_format_episode": {
      "string_format": "/torrents.php?search=%s+s%02de%02d&sort=size&order=desc",
      "replacement": [
        "title_lower",
        "season_int",
        "episode_int"
      ]
    },
    "search_url_format_movie": {
      "string_format": "/torrents.php?search=%s+%04d&sort=size&order=desc",
      "replacement": [
        "title_lower",
        "year_int"
      ]
    },
    "search_url_format_season_pack": {
    "string_format": "/torrents.php?search=%s&sort=size&order=desc",
    "replacement": [
      "title_lower"
    ]
  },
    "more_pages_indicator": "",
    "next_page_path" : "",
    "next_page_path_attr": "",
    "links_on_first_page": true,
    "drill_down_path": "",
    "drill_down_path_attr": "",
    "is_torrent": true,
    "is_direct": false,
    "links_container_path": "div.tgxtablerow",
    "link_path": "",
    "link_path_attr": "",
    "title_path": "a:has(b)",
    "title_path_attr": "",
    "details_path": "|title_path|",
    "details_path_attr": "",
    "magnet_path": "a[href^=magnet]",
    "magnet_path_attr": "href",
    "seeders_path": "div.tgxtablecell span.Seeders/Leechers b:eq(0)",
    "seeders_path_attr": "",
    "filesize_path": "div.rounded span.badge-secondary",
    "filesize_path_attr": "",
    "quality_path": "|title_path|",
    "quality_path_attr": "",
    "info_path": "|title_path|",
    "info_path_attr": ""
  }
  
