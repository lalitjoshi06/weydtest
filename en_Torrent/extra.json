{
  "name": "torrentdownload",
  "language": [
    "hi",
    "en"
  ],
  "domains": [
    "torrentdownload.ch",
    "torrentdownload.info"
  ],
  "base_url": "https://www.torrentdownload.info",
    "search_url_format_episode": {
      "string_format": "/search?q=%s+s%02de%02d",
      "replacement": [
        "title_lower",
        "season_int",
        "episode_int"
      ]
    },
    "search_url_format_movie": {
      "string_format": "/search?q=%s+%04d",
      "replacement": [
        "title_lower",
        "year_int"
      ]
    },
    "more_pages_indicator": "<div class=\"search_stat\"><span",
    "next_page_path" : "div.search_stat span.active + a",
    "next_page_path_attr": "abs:href",
    "links_on_first_page": false,
    "drill_down_path": "table.table2 div.tt-name:has(span.na) a",
    "drill_down_path_attr": "abs:href",
    "is_torrent": true,
    "is_direct": false,
    "links_container_path": "a[href^=magnet]",
    "link_path": "",
    "link_path_attr": "",
    "title_path": "/table.table3 td:has(b:contains(name)) + td",
    "title_path_attr": "",
    "details_path": "|title_path|",
    "details_path_attr": "",
    "magnet_path": "a[href^=magnet]",
    "magnet_path_attr": "href",
    "seeders_path": "/table.table3 span:contains(seeds)",
    "seeders_path_attr": "",
    "filesize_path": "/table.table3 td:has(b:contains(size)) + td",
    "filesize_path_attr": "",
    "quality_path": "|title_path|",
    "quality_path_attr": "",
    "info_path": "|title_path|",
    "info_path_attr": ""
  }
