{
  "name": "TPNL",
  "language": ["hi", "en"],
  "domains": ["torrent-paradise.ml"],
  "base_url": "https://torrent-paradise.ml",  
  "search_url_format_episode": {
    "string_format": "/api/search?q=%s+s%02de%02d",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/api/search?q=%s+%04d",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/api/search?q=%s",
    "replacement": [
      "title_lower"
    ]
  },
  "api":{
    "path_title": "text",
    "path_hash": "id",
    "path_seeders": "s",
    "path_size": "len"
  },

  "is_torrent": true,
  "is_direct": false
}
