{
  "name": "magnet4you",
  "language": ["hi", "en"],
  "domains": ["magnet4you.me", "magnet4you.nocensor.monster"],
  "base_url": "https://torrentz2.cyou",
  "search_url_format_episode": {
    "string_format": "/fullsearch?q=%s+s%02de%02d",
    "replacement": [
      "title",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/fullsearch?q=%s+%04d",
    "replacement": [
      "title",
      "year_int"
    ]
  },  
  "more_pages_indicator": "NEXT",
  "next_page_path" : "",
  "next_page_path_attr": "",
  "links_on_first_page": true,
  "drill_down_path": "",
  "drill_down_path_attr": "",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div.results, table#torrents tbody tr",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "tr td:eq(1)",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "tr td:eq(2) a[href^=magnet]",
  "magnet_path_attr": "href",
  "seeders_path": "tr td:eq(5)",
  "seeders_path_attr": "",
  "filesize_path": "tr td:eq(4)",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
