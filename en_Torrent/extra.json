{
  "name": "torrentdownload",
  "language": [
    "hi",
    "en"
  ],
  "domains": [
    "torrentdownload.ch",
    "torrentdownload.info"
  ],
  "base_url": "https://torrentdownloads.d4.re",
  "search_url_format_episode": {
    "string_format": "/search/?search=%s+s%02de%02d",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/search/?search=%s+%04d",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "more_pages_indicator": "<div class=\"search_stat\"><span",
  "next_page_path": "div.search_stat span.active + a",
  "next_page_path_attr": "abs:href",
  "links_on_first_page": false,
  "drill_down_path": "div.grey_bar3 p a[href^=/torrent/]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div.inner_container",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "h1.titl_1 span",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "div.grey_bar1 p a[href^=magnet]",
  "magnet_path_attr": "href",
  "seeders_path": "",
  "seeders_path_attr": "",
  "filesize_path": "div.grey_bar1 p span:contains(Total Size)",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
