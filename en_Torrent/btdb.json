{
	"name": "btdb",
	"language": ["en"],
	"domains": ["btdb.unblockit.ltd", "btdb.eu"],
	"base_url": "https://btdb.unblockit.ltd",
	"search_url_format_episode": {
		"string_format": "/search/%s+s%02de%02d/0/?sort=popular",
		"replacement": [
			"title_lower_dash",
			"season_int",
			"episode_int"
		]
	},
	"search_url_format_movie": {
		"string_format": "/search/%s+%04d/0/?sort=popular",
		"replacement": [
			"title_lower",
			"year_int"
		]
	},
	"search_url_format_season_pack": {
		"string_format": "/search/%s/0/?sort=popular",
		"replacement": [
			"title_lower"
		]
	},
	"more_pages_indicator": "",
	"next_page_path": "",
	"next_page_path_attr": "",
	"links_on_first_page": true,
	"drill_down_path": "",
	"drill_down_path_attr": "",
	"is_torrent": true,
	"is_direct": false,
	"links_container_path": "tr#torrent_latest_torrents",
	"link_path": "",
	"link_path_attr": "",
	"title_path": "div.torrentname a.cellMainLink",
	"title_path_attr": "",
	"details_path": "|title_path|",
	"details_path_attr": "",
	"magnet_path": "a[title=Torrent magnet link]",
	"magnet_path_attr": "href",
	"seeders_path": "td.green",
	"seeders_path_attr": "",
	"filesize_path": "td.nobr",
	"filesize_path_attr": "",
	"filesize_units": "gb",
	"quality_path": "|title_path|",
	"quality_path_attr": "",
	"info_path": "|title_path|",
	"info_path_attr": ""
}
