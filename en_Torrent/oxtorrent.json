{
  "name": "piratebay",
  "language": ["hi", "en"],
  "domains": ["apibay.org"],
  "base_url": "https://apibay.org",
  "fallback_urls": [
      "https://tpb.red",
      "https://mypiratebay.co",
      "https://mypiratebay.me",
      "https://mypiratebay.wtf",
      "https://mypiratebay.best"
    ],
  "search_url_format_episode": {
    "string_format": "/q.php?q=%s+s%02de%02d&cat=0",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/q.php?q=%s+%04d&cat=0",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/q.php?q=%s&cat=0",
    "replacement": [
      "title_lower"
    ]
  },
  "api":{
    "path_title": "name",
    "path_hash": "info_hash",
    "path_seeders": "seeders",
    "path_size": "size"
  },

  "is_torrent": true,
  "is_direct": false
}
