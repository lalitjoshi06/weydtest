{
  "name": "Badasstorrent",
  "language": [
    "hi",
    "en"
  ],
  "domains": [
    "badasstorrents.com"    
  ],
  "base_url": "https://badasstorrents.com",
  "search_url_format_episode": {
    "string_format": "/torrents/search/%s+s%02de%02d",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/torrents/search/%s+%04d",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/torrents/search/%s",
    "replacement": [
      "title_lower"
    ]
  },
  "more_pages_indicator": "",
  "next_page_path": "",
  "next_page_path_attr": "",
  "links_on_first_page": false,
  "drill_down_path": "tbody tr td:eq(2) a",
  "drill_down_path_attr": "href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div.table-responsive tbody tr",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "td:eq(1)",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "div#magnet a[href^=magnet]",
  "magnet_path_attr": "href",
  "seeders_path": "",
  "seeders_path_attr": "",
  "filesize_path": "td:eq(5)",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
