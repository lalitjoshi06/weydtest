{
  "name": "1337Api",
  "language": ["hi", "en"],
  "domains": ["src.abirxo.com"],
  "base_url": "https://src.abirxo.com",  
  "search_url_format_episode": {
    "string_format": "/1337x/%s+s%02de%02d",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/1337x/%s+%04d",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/1337x/%s",
    "replacement": [
      "title_lower"
    ]
  },
  "api":{
    "path_title": "name",    
    "path_seeders": "seeders",
    "path_size": "size",
	   "filesize_units": "gb",
	  "path_magnet": "magnet"
  },

  "is_torrent": true,
  "is_direct": false
}
