{
  "name": "1337Api",
  "language": ["hi", "en"],
  "domains": ["torscraper.herokuapp.com"],
  "base_url": "https://torscraper.herokuapp.com",  
  "search_url_format_episode": {
    "string_format": "/search1337/%s+s%02de%02d",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/search1337/%s+%04d",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/search1337/%s",
    "replacement": [
      "title_lower"
    ]
  },
  "api":{
    "path_title": "Name",    
    "path_seeders": "Seeders",
    "path_size": "Size",
"path_magnet": "Magnetlink"
  },

  "is_torrent": true,
  "is_direct": false
}
