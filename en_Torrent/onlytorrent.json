  "is_direct": false,
  "links_container_path": "div#torrents tbody tr",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "td:eq(0)",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "td:eq(9) a[href^=magnet]",
