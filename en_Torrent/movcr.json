{
  "name": "movcr",
  "language": ["hi", "en"],
  "domains": ["movcr.unbl0ck.xyz"],
  "base_url": "https://movcr.unbl0ck.xyz",
  "search_url_format_episode": {
    "string_format": "/search_results.php?search=%s-s%02de%02d&cat=41&incldead=0&inclexternal=0&lang=1&sort=seeders&order=desc",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/search_results.php?search=%s-%04d&cat=1&incldead=0&inclexternal=0&lang=1&sort=size&order=desc",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "more_pages_indicator": "<li class=\"last\">",
  "next_page_path" : "div.pagination ul li.active + li a[href]",
  "next_page_path_attr": "abs:href",
  "links_on_first_page": false,
  "drill_down_path": "tbody tr td a[href^=/torrent]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div.page-content",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "div.box-info-heading h1",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "div.torrent-detail-page div:eq(1) div ul:eq(0) li:eq(0) a",
  "magnet_path_attr": "href",
  "seeders_path": "",
  "seeders_path_attr": "",
  "filesize_path": "div.torrent-detail-page div:eq(1) div ul:eq(1) li:eq(3)",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
