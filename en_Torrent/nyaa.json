{
	"name": "nyaa",
	"language": ["en"],
	"domains": ["nyaa.net", "nyaa.si"],
	"base_url": "https://nyaa.net",
	"search_url_format_episode": {
		"string_format": "/?f=0&c=1_0&q=s+s%02de%02d&s=seeders&o=desc",
		"replacement": [
			"title_lower_dash",
			"season_int",
			"episode_int"
		]
	},
	"search_url_format_movie": {
		"string_format": "/?f=0&c=1_0&q=%s+%04d&s=seeders&o=desc",
		"replacement": [
			"title_lower",
			"year_int"
		]
	},
	"search_url_format_season_pack": {
		"string_format": "/?f=0&c=1_0&q=%s&s=seeders&o=desc",
		"replacement": [
			"title_lower"
		]
	},
	"more_pages_indicator": "",
	"next_page_path": "",
	"next_page_path_attr": "",
	"links_on_first_page": true,
	"drill_down_path": "",
	"drill_down_path_attr": "",
	"is_torrent": true,
	"is_direct": false,
	"links_container_path": "div.results tbody tr",
	"link_path": "",
	"link_path_attr": "",
	"title_path": "td.tr-links a[title]",
	"title_path_attr": "",
	"details_path": "|title_path|",
	"details_path_attr": "",
	"magnet_path": "td.tr-links a[href^=magnet]",
	"magnet_path_attr": "href",
	"seeders_path": "td:eq(2)",
	"seeders_path_attr": "",
	"filesize_path": "td:eq(3)",
	"filesize_path_attr": "",
	"filesize_units": "gb",
	"quality_path": "|title_path|",
	"quality_path_attr": "",
	"info_path": "|title_path|",
	"info_path_attr": ""
}
