{
    "name": "skytorrents",
    "language": ["hi", "en"],
    "domains": ["skytorrents.net"],
    "base_url": "https://www.skytorrents.net",
    "search_url_format_episode": {
      "string_format": "/?search=%s+s%02de%02d",
      "replacement": [
        "title_lower",
        "season_int",
        "episode_int"
      ]
    },
    "search_url_format_movie": {
      "string_format": "/?search=%s+%04d",
      "replacement": [
        "title_lower",
        "year_int"
      ]
    },
    "search_url_format_season_pack": {
      "string_format": "/?search=%s",
      "replacement": [
        "title_lower"
      ]
    },
    "more_pages_indicator": "",
    "next_page_path" : "",
    "next_page_path_attr": "",
    "links_on_first_page": true,
    "drill_down_path": "",
    "drill_down_path_attr": "",
    "is_torrent": true,
    "is_direct": false,
    "links_container_path": "tbody#results tr",
    "link_path": "",
    "link_path_attr": "",
    "title_path": "a:eq(0)",
    "title_path_attr": "",
    "details_path": "|title_path|",
    "details_path_attr": "",
    "magnet_path": "a[href^=magnet]",
    "magnet_path_attr": "href",
    "seeders_path": "td:eq(4)",
    "seeders_path_attr": "",
    "filesize_path": "td:eq(1)",
    "filesize_path_attr": "",
    "quality_path": "|title_path|",
    "quality_path_attr": "",
    "info_path": "|title_path|",
    "info_path_attr": ""
  }
  