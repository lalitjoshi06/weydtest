{
  "name": "piratebay",
  "language": ["hi", "en"],
  "domains": ["apibay.org"],
  "base_url": "https://apibay.org",
    "search_url_format_episode": {
    "string_format": "/s/?q={query}&page=0&&video=on&orderby=99",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/s/?q={query}&page=0&&video=on&orderby=99",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/s/?q={query}&page=0&&video=on&orderby=99",
    "replacement": [
      "title_lower"
    ]
  },
  "api":{
    "path_title": "name",
    "path_hash": "info_hash",
    "path_seeders": "seeders",
    "path_size": "size"
  },

  "is_torrent": true,
  "is_direct": false
}
