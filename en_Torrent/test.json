{
  "name": "kickass",
    "language": ["hi", "en"],
    "domains": ["kickass.onl", "kat.how", "kat.li", "kickass.cm", "kickass2.st", "kat.sx", "thekat.info"],
    "base_url": "https://kickass.onl",
  "search_url_format_episode": {
    "string_format": "/usearch/%s+s%02de%02d%20category:tv",
    "replacement": [
      "title",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/usearch/%s+%04d%20category:movies",
    "replacement": [
      "title",
      "year_int"
    ]
  },
   "api":{
    "path_title": "name",
    "path_hash": "info_hash",
    "path_seeders": "seeders",
    "path_size": "size"
  },

  "is_torrent": true,
  "is_direct": false
}
