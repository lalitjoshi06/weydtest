{
  "name": "torrent9",
  "language": ["hi", "en"],
  "domains": ["torrent9.to"],
  "base_url": "https://ww1.torrent9.to",
    "search_url_format_episode": {
    "string_format": "/%.1s/%s-s%02de%02d",
    "replacement": [
      "title_lower",
      "title_lower_dash",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/%.1s/%s-%d",
    "replacement": [
      "title_lower",
      "title_lower_dash",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/%.1s/%s",
    "replacement": [
      "title_lower",
      "title_lower_dash"
    ]
  },
  "api":{
    "path_title": "name",
    "path_hash": "info_hash",
    "path_seeders": "seeders",
    "path_size": "size"
  },

  "is_torrent": true,
  "is_direct": false
}
