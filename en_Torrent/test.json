{
    "name": "rarbg",
    "language": ["hi", "en"],
    "domains": ["torrentapi.org", "rarbgproxied.org"],
    "base_url": "https://rarbgproxied.org",    
    "search_url_format_episode": {
      "string_format": "/torrents.php?search=%s+S%02dE%02d",
      "replacement": [
        "title_lower",
        "season_int",
        "episode_int"
      ]
    },
    "search_url_format_movie": {
      "string_format": "/torrents.php?search=%s+%04d%",
      "replacement": [
        "title_lower",
        "year_int"
      ]
    },
    "search_url_format_season_pack": {
      "string_format": "/torrents.php?search=%s",
      "replacement": [
        "title_lower"
      ]
    },
    "api":{
        "path_to_array": "torrent_results",
        "path_title": "title",
        "path_seeders": "seeders",
        "path_size": "size",
        "path_magnet": "download"
    },
    "is_torrent": true,
    "is_direct": false
  }
