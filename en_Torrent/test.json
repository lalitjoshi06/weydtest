{
  "name": "torrent9",
  "language": ["hi", "en"],
  "domains": ["torrent9.to"],
  "base_url": "https://ww1.torrent9.to",
    "search_url_format_episode": {
      "string_format": "/search?q=%s+s%02de%02d",
      "replacement": [
        "title_lower",
        "season_int",
        "episode_int"
      ]
    },
    "search_url_format_movie": {
      "string_format": "/search?q=%s+%04d",
      "replacement": [
        "title_lower",
        "year_int"
      ]
    },
    "search_url_format_season_pack": {
      "string_format": "/search?q=%s",
      "replacement": [
        "title_lower"
      ]
    },
  "api":{
    "path_title": "name",
    "path_hash": "info_hash",
    "path_seeders": "seeders",
    "path_size": "size"
  },

  "is_torrent": true,
  "is_direct": false
}
